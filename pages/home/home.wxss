/**index.wxss**/
.page {
  margin-top: 50rpx;
	text-align: center;
  position: fixed;
  height: 100%;
  background-color: rgba(253, 249, 241, 0.527);
}

.page-wrap {
	position: relative;
  padding-top: 0rpx;
}

.logo {
    width: 100%;
    height: 375rpx;
    display: inline-block;
}

.col button{
  font-size: 16px;
  width: 90%;
	height: 200rpx;
  background-color: white;
}

.col image {
	width: 180rpx;
	height: 100%;
	display: inline-block;
	vertical-align: middle;
}

.col ::after{
  border: none;
}

.col{
  display:flex ;
  flex-direction:column;
  width: 90%;
  height: 250rpx;
  margin-left: 40rpx;
  margin-top: 10rpx;
}

.row{
    display:flex ;
    flex-direction:row;
    width: 100%;
    height: 100%;
    margin-left: 20rpx;
}

.row image {
	width: 200rpx;
	height:130rpx;
	display: inline-block;
	vertical-align: middle;
}

.rowbt{
  width: 45%;
	height: 250rpx;
  margin-left: 10rpx;
  background-color: white;
}


